{
	"User": {
		"id": {
			"int": {
				"min": 1,
				"max": 1000000
			}
		},
		"username": {
			"regexp": "[a-z]{6,16}"
		},
		"name": {
			"regexp": "[가-힣]{3,5}"
		},
		"nickname": {
			"regexp": "[A-Z][a-z][가-힣]{6,16}"
		},
		"description": {
			"regexp": "[가-힇]{6,16}"
		},
		"profileImage": {
			"regexp": "https://out\\.amor\\.com/[A-Z][a-z]{16}.jpg"
		},
		"email": {
			"regexp": "[a-z]{6,16}@[a-z]{3,5}\\.com"
		},
		"phone": {
			"regexp": "^010[0-9]{8}$"
		},
		"lastLoginAt": {
			"dateTime": {
				"min": "2022-01-01T00:00:00Z",
      				"max": "2022-12-31T23:59:59Z"
			}
		},
		"isAvailable": {
			"boolean": {}
		},
		"isSuspended": {
			"boolean": {}
		}
	}
}
